<% provide(:title, 'Users') %>
<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Community</th>
      <th>Voucher</th>
      <th>Vouched</th>
      <th colspan="6"></th>
    </tr>
  </thead>


  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.community %></td>
        <td><%= user.voucher %></td>
        <td><%= user.vouched %></td>
      </tr>
    <% end %>
  </tbody>
</table>
